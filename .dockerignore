# Exclude local database files
instance/*.db
instance/service.db

# Exclude environment files
.env
.env.local
.env.dev

# Exclude Python cache
__pycache__/
*.pyc
*.pyo
*.pyd
.Python

# Exclude IDE files
.idea/
.vscode/
*.swp
*.swo

# Exclude git
.git/
.gitignore

